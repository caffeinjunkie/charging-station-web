{
  "testEnvironment": "jsdom",
  "transform": {
    "^.+\\.(j|t)sx?$": "babel-jest",
    "^.+\\.svg$": "<rootDir>/svgTransform.ts"
  },
  "coveragePathIgnorePatterns": [
    "index.t(s|sx)",
    ".*.styles.t(s|sx)",
    ".*.config.t(s|sx)",
    ".*.type.t(s|sx)",
    ".*.graphql.t(s|sx)",
    "src/themes/*",
    "src/fixture/*",
    "src/assets/*",
    "src/root/RootNavigation/*"
  ],
  "setupFilesAfterEnv": ["./src/setupTests.ts"]
}
